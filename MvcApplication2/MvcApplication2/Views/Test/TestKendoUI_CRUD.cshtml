                            style: "text-align:center"
                        },
                        attributes: {
                            //"class": "table-cell",
                            style: "text-align:center"

                        }
                        , filterable: false
                        , sortable: false
                    },
                    {
                        field: "RecNo",
                        title: "行號",
                        width: 90,
                        headerAttributes: {
                            style: "text-align:center"
                        },
                        attributes: {
                            //"class": "table-cell",
                            style: "text-align:center"

                        }
                        , filterable: false
                        , sortable: false
                    },
                    {
