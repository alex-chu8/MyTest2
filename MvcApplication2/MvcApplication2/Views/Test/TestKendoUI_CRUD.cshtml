                            page: function () {
                                return dataSrc._page;
                            },
                            pageSize: function () {
                                return dataSrc._pageSize;
                            },
                            skip: function () {
                                return dataSrc._skip;
                            },
                            take: function () {
                                return dataSrc._take;
                            },
