                       , headerAttributes: {
                           style: "text-align:center"
                       },
                        attributes: {
                            //class:"ob-fld-boolean",
                            style: "text-align:center"
                        }
                        , filterable: false
                        , template: "<input type=\"checkbox\" name=\"VIP\" #= VIP?'checked':''  #  disabled='disabled'   />"
                    },
                    {
                        command: [
                            {
                                className: "btn-edit",
                                name: "edit",
                                text: { edit: "編輯", update: "儲存", cancel: "取消" }
                            },
                            { className: "btn-destroy", name: "destroy", text: "刪除" }
                        ],
                        title: "&nbsp;",
                        //width: 160,
                        attributes:
                            {
                                "class": "table-cell",
                                style: "text-align:center"

                            },
                    }
                ],
